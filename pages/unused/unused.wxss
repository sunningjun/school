.container{
  background-color: #f7f7f7
}

.nav_view {
  width: 95%;
  height: 40%;
  background-color: white; 
  margin-left: 2%;
  border-radius: 2px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-content: flex-start;
  flex-wrap: wrap;
  box-sizing: border-box;
  position: relative;
  top: 2%;
}

.sub_view {
  width: 20%;
  height: 45%;
  margin-left: 2%;
  margin-top: 2%;
}

.image_view {
  width: 70%;
  height: 50%;
  margin-left: 25%;
  border-radius: 50%;
}

.nav_image {
  width: 25px;
  height: 25px;
  margin-left: 20%;
  margin-top: 20%;
}

.text_view {
  font-size: 12px;
  text-align: center;
  margin-left: 20%;
  margin-top: 5%
}
.content_view{
  width: 95%;
  height: 40%;
  position: relative;
  left: 2%;
  top: 5%;

}
.leaflet{
   width: 100%;
   height: 100%;  
}
.swipper_view{
  width: 100%;
  height: 100%;
}
.footer_view{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 10%;
   background-color: white; 
}
.button{
  width: 50px;
  height: 50px;
}
.footer_button{
  width: 50%;
  height: 50%;
  position: relative;
  left: 42%;
  top: 10%;
}
