.container{
    background-color: #f7f7f7
}
.top_view{
  width: 100%;
  height: 10%;
  background-color: white
}
.scroll_view{
  width: 100%;
  height: 90%;
}
.search_view{
  width: 90%;
  height: 45%;
  position: relative;
  left: 5%;
  border-radius: 20px;
  display: flex;
  background-color: #f7f7f7;
  top: 2%
}
.input_view{
  width: 80%;
  height: 100%;
  font-size: 12px;
  margin-left: 5%
}
.search_image{
  width: 20px;
  height: 20px;
  display: block;
  margin: auto
}
.nav_view{
  width: 100%;
  height: 50%;
  display: flex;
  border-bottom: 1px solid #f7f7f7;
  background-color: white;
  margin-top: 2%
}
.header_sub_view{
  width: 25%;
  height: 50%;
  text-align: center;
  position: relative;
  top: 10%;
  font-size: 12px;
}
.header_icon{
  width: 10px;
  height: 10px;
  margin-left: 5%;  
}

.bottom_sub_view{
  width: 95%;
  height: 25%;
  background-color: white;
  margin-left: 2%;
  border-radius: 2px;
  display: flex;
  position: relative;
  top: 2%;
  margin-top: 2%
}
.bottom_image_view{
  width: 40%;
  height: 100%;
  /* background-color: black */
}
.bottom_text_view{
  width: 60%;
  height: 100%;
}
.rent_image{
  width: 90%;
  height: 90%;
  margin-left: 5%;
  margin-top: 5%
}
.bottom_text_top_view{
  width: 100%;
  height: 60%;
  border-bottom: 1px solid #f7f7f7
}
.title_class{
  font-size: 14px;
}
.price_view{
  display: flex;
  height: 60%;
  justify-content: space-between
}
.price_view_title{
  font-size: 10px;
  color: gray;
  margin-top: 2%
}
.price_view_text{
  font-size: 14px;
  color: #FF8040
}
.title_view{
  width: 100%;
  height: 40%;
}
.bottom_text_under_view{
  font-size: 10px;
  display: flex
}
.sub_item_view{
  width: 50px;
  height: 20px;
  background-color: #e6e6e6;
  text-align: center;
  justify-self: center;
  margin-top: 2%;
  border-radius: 2px;
  margin-left: 2%
}